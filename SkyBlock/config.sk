  set {SB::config::homeupgrade1} to 1000
  # > Island home 2:
  set {SB::config::homeupgrade2} to 2000
  # > Island home 3:
  set {SB::config::homeupgrade3} to 3000
  # > Island home 4:
  set {SB::config::homeupgrade4} to 4000
  # > Island home 5:
