  # > Island size 2:
  set {SB::config::sizeupgrade2} to 25000
  # > Island size 3:
  set {SB::config::sizeupgrade3} to 35000
  # > Island size 4:
  set {SB::config::sizeupgrade4} to 45000
  # > Island size 5:
  set {SB::config::sizeupgrade5} to 55000
  
  #
  # > Island home upgrades:
