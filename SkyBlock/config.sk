  add "1,lava bucket" to {SB::schematics::chest::%{_schematicname}%::*}
  add "2,ice block" to {SB::schematics::chest::%{_schematicname}%::*}
  add "50,bone meal" to {SB::schematics::chest::%{_schematicname}%::*}

