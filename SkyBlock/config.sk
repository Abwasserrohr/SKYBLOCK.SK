  #
  # > Define the xp threshold for every challenge level, the higher, the harder
  # > it gets to climb up the levels. To have everything unlocked at the beginning,
  # > set all thresholds to 0.
  set {SB::config::challenges::threshold::1} to 0
  set {SB::config::challenges::threshold::2} to 2000
  set {SB::config::challenges::threshold::3} to 5000
  set {SB::config::challenges::threshold::4} to 9000
  set {SB::config::challenges::threshold::5} to 25000

